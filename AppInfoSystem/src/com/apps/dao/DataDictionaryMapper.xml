<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.apps.dao.DataDictionaryMapper">
	
	<!-- 根据typeCode 查询 -->
	<select id="getDictionaryByType" resultType="DataDictionary">
		select * from data_dictionary where typeCode=#{typeCode}
	</select>
</mapper>